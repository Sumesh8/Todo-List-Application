{
  // For configuration settings related to authentication.
  "Auth": {
    // For secret key used for generating and validating JWT tokens.
    "JwtSecretKey": "this is my custom Secret key for authentication Secret Key to genrate token",
    // For representing the identity provider.
    "Issuer": "https://localhost:44332",
    // For representing the intended audience for the JWT tokens.
    "Audiance": "https://localhost:44332"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*"
}
